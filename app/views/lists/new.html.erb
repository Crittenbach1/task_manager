<%= form_for @list do |l| %>
  Name List:<%= l.text_field :title %><br>
  <%= l.hidden_field(:group_id, :value => @list.group_id) %>
  <%= l.fields_for :tasks do |t| %>
  Task Title:<%= t.text_field :title %><br>
  Task Content:<%= t.text_field :content %><br>
<% end %>

  <%= l.submit %>
<% end %>
